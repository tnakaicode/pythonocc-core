class IVtkTools_ShapeDataSource(vtkPolyDataAlgorithm):
    def FastTransformModeOff(self) -> None: ...
    def FastTransformModeOn(self) -> None: ...
    def GetId(self) -> IVtk_IdType: ...
    def GetShape(self) -> False: ...
    def SubShapeIDs(self) -> False: ...

class IVtkTools_ShapeObject(vtkDataObject):
    @overload
    def GetShapeSource(self) -> IVtkTools_ShapeDataSource: ...
    @overload
    def SetShapeSource(self, theDataSource: IVtkTools_ShapeDataSource) -> None: ...
    @staticmethod
    def getKey() -> False: ...

class IVtkTools_ShapePicker(vtkAbstractPropPicker):
    def GetPickedActors(self, theIsAll: Optional[bool] = false) -> False: ...
    def GetPickedShapesIds(self, theIsAll: Optional[bool] = false) -> IVtk_ShapeIdList: ...
    def GetPickedSubShapesIds(self, theId: IVtk_IdType, theIsAll: Optional[bool] = false) -> IVtk_ShapeIdList: ...
    def GetTolerance(self) -> float: ...
    def SetAreaSelection(self, theIsOn: bool) -> None: ...
    @overload
    def SetSelectionMode(self, theMode: IVtk_SelectionMode, theIsTurnOn: Optional[bool] = true) -> None: ...
    def SetTolerance(self, theTolerance: float) -> None: ...

class IVtkTools_SubPolyDataFilter(vtkPolyDataAlgorithm):
    @overload
    def AddData(self, theSet: IVtk_IdTypeMap) -> None: ...
    @overload
    def AddData(self, theIds: IVtk_ShapeIdList) -> None: ...
    def Clear(self) -> None: ...
    @overload
    def SetData(self, theSet: IVtk_IdTypeMap) -> None: ...
    @overload
    def SetData(self, theIds: IVtk_ShapeIdList) -> None: ...
    def SetDoFiltering(self, theDoFiltering: bool) -> None: ...
    def SetIdsArrayName(self, theArrayName: str) -> None: ...

class IVtkTools_DisplayModeFilter(IVtkTools_SubPolyDataFilter):
    def FaceBoundaryDraw(self) -> bool: ...
    def GetDisplayMode(self) -> IVtk_DisplayMode: ...
    def IsSmoothShading(self) -> bool: ...
    def MeshTypesForMode(self, theMode: IVtk_DisplayMode) -> IVtk_IdTypeMap: ...
    def SetDisplayMode(self, aMode: IVtk_DisplayMode) -> None: ...
    def SetDisplaySharedVertices(self, doDisplay: bool) -> None: ...
    def SetFaceBoundaryDraw(self, theToDraw: bool) -> None: ...
    def SetMeshTypesForMode(self, theMode: IVtk_DisplayMode, theMeshTypes: IVtk_IdTypeMap) -> None: ...
    def SetSmoothShading(self, theIsSmooth: bool) -> None: ...

